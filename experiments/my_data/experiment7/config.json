{
    "metadata": {
        "title": "Ablation Study Over my data (DrugBank, FooDB, BioKG) for DistMult."
    },
    "ablation": {
        "datasets": [
	  {	      
        "training": "../../../data/triplets/train.txt",
	    "validation": "../../../data/triplets/valid.txt",
	    "testing": "../../../data/triplets/test.txt"
	  }
	],
        "models":   ["DistMult"],
        "training_loops": ["slcwa"],
        "optimizers": ["Adam"],
        "losses": ["MarginRankingLoss", "BCEWithLogitsLoss"],
        "create_inverse_triples": [false],
        "model_to_training_loop_to_training_kwargs": {
            "DistMult": {
                "slcwa": {
                    "num_epochs": 30,
		            "batch_size": 512
                }
            }
        },
        "model_to_optimizer_to_optimizer_kwargs_ranges": {
            "DistMult": {
                "Adam": {
                    "lr": {
                        "type": "float",
                        "low": 0.0001,
                        "high": 0.01,
                        "scale": "log"
                    }
                }
            }
        }
    },	
    "optuna": {
        "n_trials": 1,
        "metric": "hits@10",
        "direction": "maximize",
        "sampler": "random",
        "pruner": "nop"
     }
}
