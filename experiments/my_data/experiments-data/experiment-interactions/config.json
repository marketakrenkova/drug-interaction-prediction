{
    "metadata": {
        "title": "Ablation Study Over my data (DrugBank, FooDB - only interactions) for ComplEx."
    },
    "ablation": {
        "datasets": [
	  {	      
        "training": "../../../../data/triplets/train_interactions.txt",
	    "validation": "../../../../data/triplets/valid_interactions.txt",
	    "testing": "../../../../data/triplets/test_interactions.txt"
	  }
	],
        "models":   ["ComplEx"],
        "losses": ["MarginRankingLoss"],
        "training_loops": ["slcwa"],
        "optimizers": ["Adam"],
        "create_inverse_triples": [false],
        "model_to_model_kwargs_ranges":{
            "ComplEx": {
                "embedding_dim": {
                    "type": "int",
		            "low": 500,
                    "high": 2000,
                    "q": 100
                }
            }
        },
        "model_to_training_loop_to_training_kwargs": {
            "ComplEx": {
                "slcwa": {
                    "num_epochs": 30,
		            "batch_size": 512
                }
            }
        },
        "model_to_optimizer_to_optimizer_kwargs_ranges": {
            "ComplEx": {
                "Adam": {
                    "lr": {
                        "type": "float",
                        "low": 0.0001,
                        "high": 0.01,
                        "scale": "log"
                    }
                }
            }
        }
    },	
    "optuna": {
        "n_trials": 2,
        "metric": "mrr",
        "direction": "maximize",
        "sampler": "random",
        "pruner": "nop"
     }
}
