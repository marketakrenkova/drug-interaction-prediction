{
    "metadata": {
        "title": "Ablation Study Over my data (DrugBank, FooDB)"
    },
    "ablation": {
        "datasets": [
	  {	      
        "training": "../../../../data/triplets/train_drugbank.txt",
	    "validation": "../../../../data/triplets/valid_drugbank.txt",
	    "testing": "../../../../data/triplets/test_drugbank.txt"
	  }
	],
    "models":   ["ComplEx", "RotatE"],
    "losses": ["MarginRankingLoss"],
    "training_loops": ["slcwa"],
    "optimizers": ["Adam"],
    "create_inverse_triples": [false],
    "model_to_model_kwargs_ranges":{
        "ComplEx": {
            "embedding_dim": {
                "type": "int",
                "low": 500,
                "high": 3000,
                "q": 500
            }
        },
        "RotatE": {
            "embedding_dim": {
                "type": "int",
                "low": 500,
                "high": 3000,
                "q": 500
            }
        }
    },
    "model_to_training_loop_to_training_kwargs": {
        "ComplEx": {
            "slcwa": {
                "num_epochs": 30,
                "batch_size": 512
            }
        },
        "RotatE": {
            "slcwa": {
                "num_epochs": 30,
                "batch_size": 512
            }
        }
    },
    "model_to_optimizer_to_optimizer_kwargs_ranges": {
        "ComplEx": {
            "Adam": {
                "lr": {
                    "type": "float",
                    "low": 0.0001,
                    "high": 0.01,
                    "scale": "log"
                }
            }
        },
        "RotatE": {
            "Adam": {
                "lr": {
                    "type": "float",
                    "low": 0.0001,
                    "high": 0.01,
                    "scale": "log"
                }
            }
        }
    }
    },	
    "optuna": {
        "n_trials": 2,
        "metric": "hits@10",
        "direction": "maximize",
        "sampler": "random",
        "pruner": "nop"
    }
}
