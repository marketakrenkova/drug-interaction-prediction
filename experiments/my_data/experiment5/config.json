{
    "metadata": {
        "title": "Ablation Study Over my data (DrugBank, FooDB, BioKG) for R-GCN."
    },
    "ablation": {
        "datasets": [
	  {	      
        "training": "../../../data/triplets/train.txt",
	    "validation": "../../../data/triplets/valid.txt",
	    "testing": "../../../data/triplets/test.txt"
	  }
	],
        "models":   ["RGCN"],
        "losses": ["BCEWithLogitsLoss"],
        "training_loops": ["slcwa"],
        "optimizers": ["Adam"],
        "create_inverse_triples": [true],
        "model_to_model_kwargs":{
            "RGCN": {
                "decomposition": ["bases", "blocks"],
                "edge_dropout": 0.4,
                "edge_weighting": ["inverse_in_degree", "symmetric"],
                "interaction": ["distmul", "complex"]
            }
        },
        "model_to_model_kwargs_ranges":{
            "RGCN": {
                "embedding_dim": {
                    "type": "int",
		            "low": 64,
                    "high": 256,
                    "q": 32
                }
            }
        },
        "model_to_training_loop_to_training_kwargs": {
            "RGCN": {
                "slcwa": {
                    "num_epochs": 50,
		            "batch_size": 128
                }
            }
        },
        "model_to_optimizer_to_optimizer_kwargs_ranges": {
            "RGCN": {
                "Adam": {
                    "lr": {
                        "type": "float",
                        "low": 0.001,
                        "high": 0.1,
                        "scale": "log"
                    }
                }
            }
        }
    },
    "optuna": {
        "n_trials": 1,
        "metric": "hits@10",
        "direction": "maximize",
        "sampler": "random",
        "pruner": "nop"
     }
}
