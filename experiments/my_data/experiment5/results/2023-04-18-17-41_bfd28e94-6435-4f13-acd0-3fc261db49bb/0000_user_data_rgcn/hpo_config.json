{
  "type": "hpo",
  "metadata": {
    "title": "Ablation Study Over my data (DrugBank, FooDB, BioKG) for R-GCN."
  },
  "pipeline": {
    "stopper": "NopStopper",
    "training": "../../../data/triplets/train.txt",
    "testing": "../../../data/triplets/test.txt",
    "validation": "../../../data/triplets/valid.txt",
    "dataset_kwargs": {
      "create_inverse_triples": true
    },
    "model": "RGCN",
    "model_kwargs": {
      "decomposition": [
        "bases",
        "blocks"
      ],
      "edge_dropout": 0.4,
      "edge_weighting": [
        "inverse_in_degree",
        "symmetric"
      ],
      "interaction": [
        "distmul",
        "complex"
      ]
    },
    "model_kwargs_ranges": {
      "embedding_dim": {
        "type": "int",
        "low": 64,
        "high": 256,
        "q": 32
      }
    },
    "loss": "BCEWithLogitsLoss",
    "optimizer": "Adam",
    "optimizer_kwargs_ranges": {
      "lr": {
        "type": "float",
        "low": 0.001,
        "high": 0.1,
        "scale": "log"
      }
    },
    "training_loop": "slcwa",
    "training_kwargs": {
      "num_epochs": 50,
      "batch_size": 128
    },
    "evaluator": null,
    "evaluation_kwargs": {}
  },
  "optuna": {
    "n_trials": 1,
    "timeout": 3600,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "random",
    "pruner": "nop",
    "storage": "sqlite:////work/experiments/my_data/experiment5/results/2023-04-18-17-41_bfd28e94-6435-4f13-acd0-3fc261db49bb/0000_user_data_rgcn/optuna_results.db"
  }
}