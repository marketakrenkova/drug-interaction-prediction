{
    "metadata": {
        "title": "Ablation Study Over OGB-biokg for DistMult."
    },
    "ablation": {
        "datasets": [
	  {	
	    "training": "../../../data/dataset-ogb/ogbl_biokg-my_split/train.txt",
	    "validation": "../../../data/dataset-ogb/ogbl_biokg-my_split/valid.txt",
	    "testing": "../../../data/dataset-ogb/ogbl_biokg-my_split/test.txt"
	  }
	],
        "models":   ["DistMult"],
        "losses": ["MarginRankingLoss"],
        "training_loops": ["slcwa"],
        "optimizers": ["Adam"],
        "create_inverse_triples": [false],
        "stopper": "early",
        "stopper_kwargs": {
            "frequency": 5,
            "patience": 20,
            "relative_delta": 0.002,
            "metric": "hits@10"
        },
        "model_to_model_kwargs_ranges":{
            "HolE": {
                "embedding_dim": {
                    "type": "int",
		            "low": 100,
                    "high": 1100,
                    "q": 200
                }
            }
        },
        "model_to_training_loop_to_training_kwargs": {
            "HolE": {
                "slcwa": {
                    "num_epochs": 50,
		            "batch_size": 512
                }
            }
        },
        "model_to_optimizer_to_optimizer_kwargs_ranges": {
            "HolE": {
                "Adam": {
                    "lr": {
                        "type": "float",
                        "low": 0.0001,
                        "high": 0.01,
                        "scale": "log"
                    }
                }
            }
        }
    },	
    "optuna": {
        "n_trials": 1,
        "metric": "hits@10",
        "direction": "maximize",
        "sampler": "random",
        "pruner": "nop"
     }
}
